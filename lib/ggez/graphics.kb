#

# 1000 - 1100: graphics
CODE_DRAW_TEXT = 1001
CODE_CLEAR_SCREEN = 1002

fn Print(string, x = 0, y = 0, xscale = 1, yscale = 1, fontscale = nil)
    # fontscale defaults to 16
    SEND(CODE_DRAW_TEXT, string, x, y, xscale, yscale, fontscale)
end

fn Clear(color)
    SEND(CODE_CLEAR_SCREEN, color)
end
